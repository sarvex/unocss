{
  "name": "@unocss/monorepo",
  "type": "module",
  "version": "0.50.7",
  "private": true,
  "packageManager": "pnpm@8.1.1",
  "scripts": {
    "postinstall": "esno scripts/icon-collections.ts",
    "taze": "taze minor -wIr && pnpm -r --parallel run update-post",
    "bench": "npm -C bench run bench",
    "build": "rimraf packages/*/dist && esno scripts/copy-files.ts && pnpm -r --filter=./packages/* run build && pnpm -r run build-post",
    "dev": "nr stub",
    "deploy": "nr build && npm -C docs run docs:build && npm -C playground run build && npm -C interactive run build",
    "interactive": "npm -C interactive run dev",
    "docs": "npm -C docs run docs:dev",
    "inspector": "npm -C packages/inspector run dev",
    "play": "npm -C playground run dev",
    "lint": "eslint --cache .",
    "lint:fix": "nr lint --fix",
    "release": "bumpp -r",
    "size": "esno scripts/size.ts",
    "stub": "pnpm -r --filter=./packages/* --parallel run stub",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:update": "vitest -u",
    "test:ci": "nr build && nr typecheck && nr lint && nr test"
  },
